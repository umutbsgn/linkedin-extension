{
    "version": 2,
    "buildCommand": "cd vercel-backend && npm install",
    "routes": [
        { "src": "/api/(.*)", "dest": "/vercel-backend/index.js" },
        { "src": "/(.*)", "dest": "/vercel-backend/index.js" }
    ],
    "github": {
        "enabled": true,
        "silent": false
    }
}