{
    "version": 2,
    "builds": [
        { "src": "vercel-backend/index.js", "use": "@vercel/node" }
    ],
    "routes": [
        { "src": "/api/(.*)", "dest": "/vercel-backend/index.js" },
        { "src": "/(.*)", "dest": "/vercel-backend/index.js" }
    ],
    "github": {
        "enabled": true,
        "silent": false
    }
}